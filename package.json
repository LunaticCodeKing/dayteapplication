    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/platform-socket.io": "^7.4.4",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.9",
    "@nestjs/websockets": "^7.4.4",
    "aws-sdk": "^2.824.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "fcm-node": "^1.3.0",
    "firebase-admin": "^9.4.2",
    "helmet": "^4.2.0",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.8",
    "multer": "^1.4.2",
